
{{#each this}}
    <li class="gallery-card">
        <a class="gallery-card-link" href="{{largeImageURL}}">
            <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="300" height="300" />
            <div class="info">
                <p class="info-item">
                    <b>Likes: {{likes}}</b>
                </p>
                <p class="info-item">
                    <b>Views: {{views}}</b>
                </p>
                <p class="info-item">
                    <b>Comments: {{comments}}</b>
                </p>
                <p class="info-item">
                    <b>Downloads: {{downloads}}</b>
                </p>
            </div>
        </a>
    </li>
{{/each}}